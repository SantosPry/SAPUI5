{
    "_version": "1.65.0",
    "sap.app": {
        "id": "master.logaligroup.empleados",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.5",
            "toolsId": "40b9badf-6aca-42bc-be8d-1b2ca599229f"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.133.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "master.logaligroup.empleados.i18n.i18n"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlAggregation": "pages",
                "controlId": "app",
                "transition": "slide",
                "type": "View",
                "viewType": "XML",
                "path": "master.logaligroup.empleados.view",
                "async": true,
                "viewPath": "master.logaligroup.empleados.view"
            },
            "routes": [
                {
                    "name": "RouteMain",
                    "pattern": ":?query:",
                    "target": [
                        "TargetMain"
                    ]
                },
                {
                    "name": "RouteDetails",
                    "pattern": "Employee(ID)",
                    "target": [
                        "TargetDetails"
                    ]                    
                }
            ],
            "targets": {
                "TargetContainer": {
                    "viewName": "Container"

                },

                "TargetMain": {
                    "parent": "TargetContainer",
                    "controlId": "container",
                    "controlAggregation": "beginColumnPages",
                    "name": "Main"
                },
                "TargetDetails": {
                    "parent": "TargetContainer",
                    "controlId": "container",
                    "controlAggregation": "midColumnPages",
                    "name": "Details"
                }                
            }
        },
        "rootView": {
            "viewName": "master.logaligroup.empleados.view.App",
            "type": "XML",
            "id": "App"
        }
    }
}