{
    "_version": "1.65.0",
    "sap.app": {
        "id": "cursologaligrup.invoicessantos",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.3",
            "toolsId": "3d6c87ed-123a-430c-9722-545fb8ef4fe0"
        },
        "dataSources": {
            "servicioOdatads" : {
                "type": "OData",
                "uri": "/v2/northwind/northwind.svc/",
                "settings": {
                    "odataVersion": "2.0"

                }

            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.132.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "cursologaligrup.invoicessantos.i18n.i18n"
                }
            },
            "invoicessantos" : {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/invoicessantos.json"
            },

            "serviceOdata" : {
                "dataSource": "servicioOdatads",
                "preload": true               

            }

        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlAggregation": "pages",
                "controlId": "idApp",
                "transition": "slide",
                "type": "View",
                "viewType": "XML",
                "path": "cursologaligrup.invoicessantos.view",
                "async": true,
                "viewPath": "cursologaligrup.invoicessantos.view"
            },
            "routes": [
                {
                    "name": "RouteApp",
                    "pattern": ":?query:",
                    "target": [
                        "TargetApp"
                    ]
                },
                {
                    "name": "RouteDetalle",
                    "pattern": "Detalle",
                    "target":  "TargetDetalle"
                  
                }                
            ],
            "targets": {
                "TargetApp" : {
                   "viewName": "App"
                },
                "TargetDetalle" : {
                    "viewName" :"Detalle"

                }                
            }
        },
        "rootView": {
            "viewName": "cursologaligrup.invoicessantos.view.App",
            "type": "XML",
            "id": "App"
        }
    }
}